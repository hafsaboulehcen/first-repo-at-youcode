#include <stdio.h>
#include <math.h>

int main()
{
    float a,b,c,m;
    printf("entrer a:");
    scanf("%f",&a);
    printf("entrer b:");
    scanf("%f",&b);
    printf("entrer c:");
    scanf("%f",&c);
    m = pow((a * b * c ) , (0.33));
    printf("la moyenne geometrique est:%f",m );

    return 0;
}

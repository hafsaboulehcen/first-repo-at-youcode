#include<stdio.h>
#include <math.h>

int main()
{
    float a,b;
    printf("saisir la valeur de a:");
    scanf("%f",&a);
    printf("saisir la valeur de b:");
    scanf("%f",&b);
    printf("resultat est :%f", a + b);
    printf("resultat est :%f", a * b);
    printf("resultat est :%f", a - b);
    printf("resultat est :%f", a / b);

    return 0;
}
